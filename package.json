{
  "name": "plantify-landing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --dist-dir dist",
    "build:prod": "EXPO_PUBLIC_RECAPTCHA_SITE_KEY=$RECAPTCHA_SITE_KEY EXPO_PUBLIC_APP_STORE_URL=$APP_STORE_URL EXPO_PUBLIC_PLAY_STORE_URL=$PLAY_STORE_URL parcel build index.html --dist-dir dist",
